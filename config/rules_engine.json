{
  "version": "2025.3",
  "description": "Full Logic: Hard Gates, Prioritization, and Scoring",
  
  "hard_gates": {
    "biz_exp": "No",
    "financial_status": "No, I do not have the resources to cover the clinic's working capital",
    "location_status": ["No, but I have found ideal locations", "No, I would need assistance"],
    "clinic_conversion_failures": {
      "is_llc": "No",
      "size": "Less than 600 sq. ft",
      "kmpdc": "No",
      "convert_willingness": "No"
    }
  },

  "soft_rejection_logic": {
    "experience": ["None", "1-2 Years"],
    "financial": ["I need a loan", "I'm still planning"],
    "location": ["No, but I have found ideal locations", "No, I would need assistance"]
  },

  "prioritization": {
    "rank_1_criteria": { "location_status": "Yes, I own or lease a location" },
    "rank_2_criteria": { "financial_status": "I have adequate resources" }
  },

  "scoring_model": {
    "gate_1": [
      {
        "id": "profession_score",
        "input_field": "current_profession",
        "weight": 0.04,
        "mapping": {
          "Clinical Officer": 1.0,
          "Medical Doctor": 1.0,
          "Nurse": 0.75,
          "Lab Technician": 0.50,
          "Pharm technician": 0.50,
          "Other": 0.25,
          "_default": 0.0
        }
      },
      {
        "id": "experience_score",
        "input_field": "experience_years",
        "weight": 0.04,
        "mapping": {
          "3-4+ Years": 1.0,
          "3-4 Years": 1.0,
          "2-3 years": 0.75,
          "1-2 Years": 0.50,
          "None": 0.0,
          "_default": 0.0
        }
      },
      {
        "id": "business_score",
        "input_field": "has_business_exp",
        "weight": 0.04,
        "mapping": {
          "Yes": 1.0,
          "No": 0.0,
          "_default": 0.0
        }
      },
      {
        "id": "proximate_fit_score",
        "input_field": "location_county_input",
        "weight": 0.04,
        "logic_type": "territory_match"
      },
      {
        "id": "offering_fit_score",
        "input_field": "location_status_input",
        "weight": 0.04,
        "logic_type": "pass_through",
        "default_value": 1.0
      }
    ]
  },

  "thresholds": {
    "max_score": 0.20,
    "classifications": [
      { "label": "Ideal Fit", "min_score": 0.20 },
      { "label": "Good Fit", "min_score": 0.19 },
      { "label": "Fair/Conditional Fit", "min_score": 0.17 },
      { "label": "Not A Fit", "min_score": 0.00 }
    ]
  },

  "readiness_maps": {
    "financial": {
      "ready": ["cash available", "adequate resources"],
      "nurture": ["loan", "savings", "planning"]
    },
    "location": {
      "ready": ["own", "lease"],
      "nurture": ["potential communities", "assistance"]
    }
  }
}